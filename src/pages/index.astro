---
import Hero from '../components/Hero.astro';
import Layout from '../layouts/Layout.astro';
import Proyectos from '../components/Proyectos.astro';
import VocalRemoverDemo from '../components/VocalRemoverDemo.astro';
import SectionContainer from '../components/SectionContainer.astro';
import Precios from '../components/Precios.astro';
import Blog from '../components/Blog.astro';
import Contacto from '../components/Contacto.astro';
import Faqs from '../components/faqs/Faqs.astro';
---

<Layout title="Landing Page Winkermind">
	<SectionContainer id="hero" className='2xl:pt-24 lg:pt-14 pb-10'>
		<Hero id="hero"/>
	</SectionContainer>
	<SectionContainer id="proyectos">
		<Proyectos />
		<VocalRemoverDemo />
	</SectionContainer>

	<SectionContainer id="precios">
		<Precios />
	</SectionContainer>

	<SectionContainer id="blog">
		<Faqs />
		<Blog />
	</SectionContainer>

	<SectionContainer id="contacto">
		<Contacto />
	</SectionContainer>
</Layout>
